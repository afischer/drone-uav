{
  "extends": ["airbnb-base"],
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["warn"] }],
    "import/no-dynamic-require": 0,
    "object-curly-newline": ["error", {
        "ObjectPattern": { "multiline": true, "minProperties": 6 }
    }]
  }
}
